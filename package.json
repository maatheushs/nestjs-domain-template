{
  "name": "example-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:report": "eslint \"{src,apps,libs,test}/**/*.ts\" --output-file report/eslint-report.json --format json",
    "pre:test": "DOTENV_CONFIG_PATH=.env.test npm run typeorm:ds schema:drop && DOTENV_CONFIG_PATH=.env.test npm run typeorm:ds migration:run && npm run fixtures:load",
    "test": "npm run pre:test && DOTENV_CONFIG_PATH=.env.test jest --config ./test/jest.json",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest.json --runInBand",
    "typeorm": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:ds": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js -d ./src/infra/database/datasource.ts",
    "fixtures:load": "DOTENV_CONFIG_PATH=.env.test fixtures-ts-node-commonjs --require=tsconfig-paths/register -d ./src/infra/database/datasource.ts load test/utils/fixtures/",
    "dependency-graph": "npx depcruise src --include-only '^src' --config --output-type dot | dot -T svg > report/dependency-graph.svg"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/js-yaml": "^4.0.5",
    "bull": "^4.10.1",
    "buscadorcep": "^2.1.0",
    "class-validator": "^0.13.2",
    "jest-html-reporter": "^3.7.0",
    "joi": "^17.7.0",
    "js-yaml": "^4.1.0",
    "jwks-rsa": "^3.0.0",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "typeorm-transactional": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dependency-cruiser": "^11.18.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-junit": "^14.0.1",
    "nock": "^13.2.9",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typeorm-fixtures-cli": "^3.0.1",
    "typescript": "^4.7.4"
  }
}
